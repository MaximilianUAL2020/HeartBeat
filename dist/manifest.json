{
  "name": "210409_Build_1",
  "version": "1.0.0",
  "description": "Learning to code chrome extensions!",
  "permissions": ["tabs", "storage", "background", "unlimitedStorage"],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48-on.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "css": ["css/style.css"],
      "js": ["js/content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "web_accessible_resources": [
    "css/prompt.css",
    "prompt.html",
    "icons/heart_0.png",
    "icons/heart_1.png",
    "icons/heart_2.png",
    "icons/heart_3.png",
    "icons/heart_4.png",
    "icons/heart_5.png"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/48-off.png"
  },
  "manifest_version": 2
}
